{
  "manifest_version": 2,

  "name": "iSeek Exporter",
  "description": "This extension formats and downloads the iSeek search results for further processing",
  "version": "1.0",

  "background": {
    "scripts": ["tracker.js"],
    "persistent": false
  },

  "page_action": {
    "default_icon": {
      "32": "iseek32.png"
    },
    "default_popup": "popup.html",
    "default_title": "iSeek Extractor"
  },
  "permissions": [
    "declarativeContent",
    "debugger",
    "tabs",
    "alarms"
  ]
}
